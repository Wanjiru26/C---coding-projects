//c++ program for a 3D Array
//author: Wanjiru Machehu
//reg.no:BSCIT-05-0100/2024

#include <iostream>
using namespace std;

int main() {
  // Declare a 3D array of integers with 2 matrices, 2 rows, and 3 columns
  int marks[2][2][3]= {
      {{10,11,12},{16,17,18}},
      {{13,14,15},{19,20,21}}
  };
int i, j, k;

  for ( i = 0; i < 2; i++) {
    for (j = 0; j < 2; j++) {
      for (k = 0; k < 3; k++) {
        cout << marks[i][j][k] << "\t";
      }
      cout << endl;
    }
     cout << endl;
  }

  return 0;
}
