#include <iostream>
using namespace std;

int main() {
  // Declare an array and assign values of integers with 5 elements
  int numbers[5]={10,20,30,40,50};
int i;
  for ( i = 0; i < 5; i++) {
cout << numbers[i] <<endl;
  }
  
  return 0;
}


